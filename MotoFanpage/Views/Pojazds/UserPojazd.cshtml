@model IEnumerable<MotoFanpage.Models.Ogólne.Pojazd>
@{
    ViewBag.Title = "UserPojazd";
}


<h2>MY Vehicles</h2>

<table class="table">
    <tr>
        <th>
            Marka
        </th>
        <th>
            Model
        </th>

        <th>
            Rok Produkcji
        </th>
        <th>
            Moc
        </th>
        <th>
        Typ
        </th>



        <th></th>
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Marka)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Model)

    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Przebieg)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.MOC)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TypeP)
    </td>


    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |

        @Html.ActionLink("Delete", "Delete", new { id = item.ID })
    </td>
</tr>
    }

</table>

